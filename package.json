{
    "name": "@meshagent/meshagent-tailwind",
    "version": "0.25.1",
    "description": "Meshagent Tailwind Components",
    "homepage": "https://github.com/meshagent/meshagent-tailwind",
    "scripts": {
        "build:js": "node build.js",
        "build:css": "tailwindcss -c tailwind.config.js -i styles.css -o dist/index.css --minify",
        "build:types": "tsc -p tsconfig.esm.json && tsc -p tsconfig.cjs.json",
        "build": "npm run build:js && npm run build:types && npm run build:css"
    },
    "author": "Meshagent Software",
    "license": "Apache-2.0",
    "module": "dist/esm/index.js",
    "main": "./dist/esm/index.js",
    "types": "dist/esm/index.d.ts",
    "exports": {
        ".": {
            "module": "./dist/esm/index.js",
            "default": "./dist/cjs/index.js"
        },
        "./dist/index.css": "./dist/index.css",
        "./package.json": "./package.json"
    },
    "files": [
        "dist",
        "!dist/**/test",
        "LICENSE",
        "README.md",
        "CHANGELOG.md"
    ],
    "dependencies": {
        "@meshagent/meshagent": "^0.25.1",
        "@meshagent/meshagent-react": "^0.25.1",
        "@radix-ui/react-avatar": "^1.1.10",
        "@radix-ui/react-checkbox": "^1.3.2",
        "@radix-ui/react-dialog": "^1.1.14",
        "@radix-ui/react-icons": "^1.3.2",
        "@radix-ui/react-label": "^2.1.7",
        "@radix-ui/react-progress": "^1.1.7",
        "@radix-ui/react-radio-group": "^1.3.7",
        "@radix-ui/react-select": "^2.2.5",
        "@radix-ui/react-slot": "^1.2.3",
        "base-64": "^1.0.0",
        "class-variance-authority": "^0.7.1",
        "clsx": "^2.1.1",
        "esbuild-plugin-alias": "^0.2.1",
        "lucide-react": "^0.525.0",
        "next-themes": "^0.4.6",
        "react": "^19.1.0",
        "react-dom": "^19.1.0",
        "react-hook-form": "^7.61.1",
        "react-markdown": "^10.1.0",
        "rehype-highlight": "^7.0.2",
        "rehype-sanitize": "^6.0.0",
        "remark-gfm": "^4.0.1",
        "sonner": "^2.0.6",
        "tailwind-merge": "^3.3.1",
        "uuid": "^11.1.0",
        "yjs": "^13.6.28"
    },
    "devDependencies": {
        "@eslint/js": "^9.29.0",
        "@tailwindcss/aspect-ratio": "^0.4.2",
        "@tailwindcss/cli": "^4.1.11",
        "@tailwindcss/forms": "^0.5.10",
        "@tailwindcss/postcss": "^4.1.11",
        "@tailwindcss/typography": "^0.5.16",
        "@tailwindcss/vite": "^4.1.11",
        "@types/node": "^24.0.13",
        "@types/react": "^19.1.8",
        "@types/react-dom": "^19.1.6",
        "@vitejs/plugin-react": "^4.5.2",
        "autoprefixer": "^10.4.21",
        "esbuild": "^0.25.6",
        "eslint": "^9.32.0",
        "eslint-plugin-react-hooks": "^5.2.0",
        "eslint-plugin-react-refresh": "^0.4.20",
        "globals": "^16.2.0",
        "tailwindcss": "^4.1.11",
        "tw-animate-css": "^1.3.5",
        "typescript": "~5.8.3",
        "typescript-eslint": "^8.34.1",
        "vite": "^7.0.0"
    },
    "overrides": {
        "rollup": "4.52.5"
    }
}
